{
  "name": "score-gpt",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "next build",
    "dev": "run-p server start",
    "fix": "npx prettier . --write",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" && echo '✅ Linting passed!'",
    "lint:fix": "eslint \"src/**/*.{js,jsx,ts,tsx}\" --fix",
    "prebuild": "npm run lint && npm run typecheck",
    "server": "node server",
    "start": "next dev",
    "test": "next test",
    "typecheck": "tsc --noEmit && echo '✅ Type check passed!'"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "jest": {
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  },
  "dependencies": {
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^13.5.0",
    "cors": "^2.8.5",
    "next": "^15.3.3",
    "openai": "^4.0.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "typescript": "^5.4.2",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/node": "^20.11.30",
    "@types/react": "^18.2.67",
    "@types/react-dom": "^18.2.22",
    "@types/testing-library__jest-dom": "^5.14.9",
    "dotenv": "^16.5.0",
    "eslint-config-prettier": "^10.1.5",
    "express": "^5.1.0",
    "http-proxy-middleware": "^3.0.5",
    "npm-run-all": "^4.1.5",
    "prettier": "3.5.3",
    "request": "^2.88.2"
  }
}
